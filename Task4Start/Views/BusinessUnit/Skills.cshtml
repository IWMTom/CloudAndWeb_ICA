@model List<Task4Start.Models.SkillVM>

@{
    ViewBag.Title = "Skills";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Skills</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().skillCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().skillDescription)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.skillCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.skillDescription)
        </td>
    </tr>
}

</table>
